  "slider-layout#beneficios-prime": {
    "title": "CarosselConfig",
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 4,
        "phone": 2
      },
      "autoplay": {
        "timeout": 2000,
        "stopOnHover": true
      },
      "arrowSize": {
        "desktop": 20,
        "tablet": 10,
        "phone": 10
      },
      "infinite": true,
      "usePagination": true,
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "mobileOnly",
      "blockClass": "beneficios"
    }
  },
  //cabeçalho conheca
  "rich-text#HeaderConheca": {
    "title": "HeaderConheca",
    "props": {
      "text": "### conheça o Clube Supernosso Prime",
      "blockClass": "textConheca"
    }
  },
  "rich-text#subHeaderConheca": {
    "title": "subHeaderConheca",
    "props": {
      "text": "Garanta **entrega grátis**, descontos e **ofertas exclusivas** em todo o site, app, lojas físicas e parceiros com o nosso serviço de assinatura em um pagamento único. [Clique aqui](#iFrameResizer0) e confira o melhor plano Prime para você!",
      "blockClass": "textConheca"
    }
  },
